{
  "name": "@worldcoin/idkit-vue",
  "version": "0.0.0",
  "main": "./build/@worldcoin/idkit-vue.js",
  "module": "./build/@worldcoin/idkit-vue.js",
  "umd": "./build/@worldcoin/idkit-vue.umd.cjs",
  "unpkg": "./build/@worldcoin/idkit-vue.cjs",
  "jsdelivr": "./build/@worldcoin/idkit-vue.cjs",
  "cdn": "./build/@worldcoin/idkit-vue.min.js",
  "exports": {
    ".": {
      "import": "./build/@worldcoin/idkit-vue.js",
      "require": "./build/@worldcoin/idkit-vue.umd.cjs"
    }
  },
  "types": "build/index.d.ts",
  "type": "module",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "lint": "bun run lint:prettier && bun run lint:eslint && bun run lint:css",
    "lintfix": "bun run lint:prettier:fix && bun run lint:eslint:fix && bun run lint:css:fix",
    "lint:js": "bun run lint:eslint && bun run lint:prettier",
    "lint:eslint": "eslint \"{,!(node_modules|build)/**/}*.{js,ts,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|build)/**/}*.{js,ts,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|build)/**/}*.{js,ts,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|build)/**/}*.{js,ts,vue}\"",
    "lint:css": "stylelint \"{,!(node_modules|build)/**/}*.{css,scss,vue}\"",
    "lint:css:fix": "stylelint --fix \"{,!(node_modules|build)/**/}*.{css,scss,vue}\""
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.4.0",
    "@vinayakkulkarni/prettier-config-vue": "^1.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^9.24.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-prettier": "^5.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "keywords": [
    "identity",
    "ID",
    "web3",
    "proof-of-personhood",
    "sybil resistance"
  ],
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worldcoin/idkit-js.git",
    "directory": "packages/vue"
  },
  "private": false,
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/worldcoin/idkit-js/issues"
  },
  "homepage": "https://docs.worldcoin.org/id/idkit"
}
